<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:localui="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.letv.wallet.common.widget.LabeledTextView
        android:id="@+id/tv_product_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        localui:label_text="@string/label_product_name"
        />

    <com.letv.wallet.common.widget.LabeledTextView
        android:id="@+id/tv_sn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        localui:label_text="@string/label_order_serialnumber"
        />

    <com.letv.wallet.common.widget.LabeledTextView
        android:id="@+id/tv_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        localui:label_text="@string/mobile_order_desc_label_phonenumber"
        />
</LinearLayout>